<body class="page-header-fixed page-quick-sidebar-over-content page-sidebar-closed-hide-logo page-container-bg-solid">

    @Html.Partial("~/Views/Admin/_PartialHeader.cshtml")
    <div class="clearfix"></div>
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        @Html.Partial("~/Views/Admin/_Partial_NavigationLeft.cshtml")
        @RenderBody()
        @Html.Partial("~/Views/Admin/_Partial_NavigationRight.cshtml")
    </div>
        @Html.Partial("~/Views/Admin/_PartialFooter.cshtml")
        <!--ITL Javascript-->
        <!--Plugin Mandatory-->
        <script src="@Url.Content("~/Content/assets/global/plugins/jquery.min.js")"></script>
        <script src="@Url.Content("~/Content/assets/global/plugins/jquery-migrate.min.js")"></script>
        <!-- IMPORTANT! Load jquery-ui.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
