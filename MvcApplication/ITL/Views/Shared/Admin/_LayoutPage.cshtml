        <script src="@Url.Content("~/Content/assets/global/scripts/metronic.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/assets/admin/layout/scripts/layout.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/assets/admin/layout/scripts/quick-sidebar.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/assets/admin/layout/scripts/demo.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/assets/admin/pages/scripts/index.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/assets/admin/pages/scripts/tasks.js")" type="text/javascript"></script>

        <script>
            jQuery(document).ready(function () {
                Metronic.init(); // init metronic core componets
                Layout.init(); // init layout
                QuickSidebar.init(); // init quick sidebar
                Demo.init(); // init demo features
                Index.init();
                Index.initDashboardDaterange();
                Index.initJQVMAP(); // init index page's custom scripts
                Index.initCalendar(); // init index page's custom scripts
                Index.initCharts(); // init index page's custom scripts
                Index.initChat();
                Index.initMiniCharts();
                Tasks.initDashboardWidget();
            });
        </script>
